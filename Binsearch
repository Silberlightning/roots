def BinSearch(n):
  low = 1
  mid = 0
  high = n
  while (low <= high):
    mid = ((low + high)// 2)
    if (n == (mid**2)) :
      item = mid
      return item
    elif (n < (mid**2)):
      high = mid
    else:
      low = mid
    if ((high - low) == 1) :
      return ("Square root of " + str(n) + " is not an integer")
n = 225
print(BinSearch(n))
